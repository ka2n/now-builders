{
  "version": 2,
  "builds": [
    { "src": "with-bundle/index.js", "use": "@ka2n/now-node-server" },
    { "src": "without-bundle/index.js", "use": "@ka2n/now-node-server", "config": { "bundle": false } }
  ],
  "probes": [
    { "path": "/with-bundle", "mustContain": "RANDOMNESS_PLACEHOLDER:with-bundle" },
    { "path": "/without-bundle", "mustContain": "RANDOMNESS_PLACEHOLDER:without-bundle" }
  ]
}
