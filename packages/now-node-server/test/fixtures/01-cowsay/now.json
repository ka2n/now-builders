{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@ka2n/now-node-server" },
    { "src": "subdirectory/index.js", "use": "@ka2n/now-node-server" }
  ],
  "probes": [
    { "path": "/", "mustContain": "cow:RANDOMNESS_PLACEHOLDER" },
